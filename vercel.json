{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://semydev.crm4.dynamics.com/"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://semydev.crm4.dynamics.com/"
        }
      ]
    }
  ]
}
